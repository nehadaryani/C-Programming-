#include<stdio.h>
int main(){
    for (int i = 0; i < 5; i++)
    {
        printf("Hello world\n");
    }
    for (int i = 5; i<=10; i++)
    {
       printf("%d\n",i);
    }
    for (int i = 10; i >=5; i=i-1)
    {
         printf("%d\n",i);
    }
    for(int i=0;i<=10;i++){
        printf("%d ",i);
    }
    int i=1;
    printf("%d ",i++);
    printf("%d ",i--);
    printf("%d ",--i);
    printf("%d ",++i); 
    for (float i = 2.1; i <=2.9; i++)
    {
        printf("%f\n",i);
    }
    for (char ch='a';ch<='z'; ch++){
        printf("%c ",ch);
        
    }
    printf("\n");
    for (char ch='A';ch<='Z'; ch++){
        printf("%c ",ch);
    }
    
    return 0;
}
