#include<stdio.h>
int main(){
    int a,b,c,n;
    printf("Enter n=");
    scanf("%d",&n);
    printf("Enter a=");
    scanf("%d",&a);
    printf("Enter b=");
    scanf("%d",&b);
    printf("Enter c=");
    scanf("%d",&c);
    int avg=(a+b+c)/n;
    printf("The Average result is=%d",avg);
    return 0;
    
}
