#include<stdio.h>
int main(){
    printf("%d\n",4>=4); // 1 stands for true
    printf("%d\n",3>=4); // 0 stands for false 
    printf("%d\n",5<4);
    printf("%d\n",10>4);
    printf("%d\n",1!=4); //not equal to
    printf("%d\n",10==4); // equal to*/
    printf("%d\n",(4>1)&&(2<6));
    printf("%d\n",(0>1)&&(2<6));  //if any 1 input is false then overall output is false.
    printf("%d\n",!((4>1)&&(2<6)));
    printf("%d\n",!((4>8)&&(2<6)));
    return 0;
}
