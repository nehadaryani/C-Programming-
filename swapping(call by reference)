#include<stdio.h>
void swap(int *a,int *b){
    int t=*a;
    *a=*b;
    *b=t;
    //printf("\nAfter swapping a=%d and b=%d ",a,b);
}
int main(){
    int a,b,t;
    printf("Enter a:");
    scanf("%d",&a);
    printf("Enter b:");
    scanf("%d",&b);
    printf("Numbers before swapping is a= %d and b= %d",a,b);
    swap(&a,&b);
    printf("\nNumbers after swapping is a= %d and b= %d",a,b);
    return 0;
}
