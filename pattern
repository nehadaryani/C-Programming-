#include<stdio.h>
int main(){
    int n,m;    //n=no of rows, m=no of columns
    printf("Enter n(rows):");
    scanf("%d",&n);
     printf("Enter m(columns):");
    scanf("%d",&m);
    
    for (int i = 1; i<=n; i++)
    {
        for (int j = 1; j<=m; j++)
        {
           printf("*",j);
        }
    printf("\n");
        
    }
    
    return 0;
}
